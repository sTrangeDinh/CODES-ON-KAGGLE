   "source": [
    "# ============================================\n",
    "# CELL 1.5 — Letterboxing\n",
    "# ============================================\n",
    "class SquarePad:\n",
    "    def __call__(self, image):\n",
    "        w, h = image.size\n",
    "        max_wh = max(w, h)\n",
    "        p_left, p_top = (max_wh - w) // 2, (max_wh - h) // 2\n",
    "        p_right, p_bottom = max_wh - w - p_left, max_wh - h - p_top\n",
    "        return F.pad(image, (p_left, p_top, p_right, p_bottom), 0, 'constant') "
   ]
  },
